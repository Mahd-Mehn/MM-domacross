JWT_PRIVATE_KEY_BASE64=
JWT_PUBLIC_KEY_BASE64=
JWT_ISSUER=domacross
JWT_AUDIENCE=domacross-users
JWT_TTL_SECONDS=3600

# External APIs
ALCHEMY_API_KEY=

# Valuation
VALUATION_MODEL_VERSION=v1.0

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8001
